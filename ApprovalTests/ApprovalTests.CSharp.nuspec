		<authors>Llewellyn Falco &amp; Dan Gilkerson</authors>
		<projectUrl>https://github.com/approvals/ApprovalTests.Net</projectUrl>
		<iconUrl>http://approvaltests.sourceforge.net/sites/all/themes/corolla/logo.png</iconUrl>
		<dependencies>
    	    <dependency id="ApprovalUtilities"/>
    	</dependencies>
	</metadata>
	<files>
		<file src="bin\Debug\ApprovalTests.dll" target="lib\net40" /> 
		<file src="bin\Debug\ApprovalTests.pdb" target="lib\net40" /> 
	</files>

</package>