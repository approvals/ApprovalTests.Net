    <tags>Approvals ApprovalTests ApprovalTest Test Verification Verify Approve Testing</tags>
    <dependencies>
      <group targetFramework=".NETFramework3.5">
        <dependency id="ApprovalUtilities" />
      </group>
    </dependencies>
  </metadata>
</package>
