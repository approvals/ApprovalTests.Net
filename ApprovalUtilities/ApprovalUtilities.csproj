<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net45</TargetFrameworks>
    <AssemblyOriginatorKeyFile>$(SolutionDir)\ApprovalsKeyPair.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>true</SignAssembly>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <GenerateAssemblyInfo>true</GenerateAssemblyInfo>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Common helper classes developed with ApprovalTests</Description>
    <DebugType>full</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="AlphaFS" Version="2.1.3" />
    <PackageReference Include="NHibernate" Version="4.*" PrivateAssets="All" />
    <PackageReference Include="Mvc2" Version="2.*" PrivateAssets="All" />
    <PackageReference Include="Fody" Version="2.4.2" PrivateAssets="All" />
    <PackageReference Include="Publicize.Fody" Version="1.4.0" PrivateAssets="All" />
    <PackageReference Include="Virtuosity.Fody" Version="1.20.2" PrivateAssets="All" />
    <Reference Include="PresentationCore" Pack="False" />
    <Reference Include="PresentationFramework" Pack="False" />
    <Reference Include="System.Core" Pack="False" />
    <Reference Include="System.Data.Entity" Pack="False" />
    <Reference Include="System.Management" Pack="False" />
    <Reference Include="System.Xml.Linq" Pack="False" />
    <Reference Include="System.Data" Pack="False" />
    <Reference Include="System.Xml" Pack="False" />
    <Reference Include="WindowsBase" Pack="False" />
  </ItemGroup>
</Project>